# Supabase Configuration
# Получите значения на: https://supabase.com/dashboard/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Контакты компании
# Укажите URL вашего Telegram и номер WhatsApp
NEXT_PUBLIC_TELEGRAM_URL=https://t.me/your_username
NEXT_PUBLIC_WHATSAPP_NUMBER=79991234567

# Unsplash API Configuration (опционально)
# Получите ключ на: https://unsplash.com/developers
UNSPLASH_ACCESS_KEY=your_unsplash_key_here

# Pexels API Configuration (опционально)
# Получите ключ на: https://www.pexels.com/api/
PEXELS_API_KEY=your_pexels_key_here

# Telegram Bot Configuration
# Получите токен у @BotFather в Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_here
# ID чата куда отправлять заявки (ваш Telegram ID или ID группы)
# Чтобы узнать ваш ID: напишите @userinfobot в Telegram
TELEGRAM_CHAT_ID=your_chat_id_here

# AI Services для поиска по картинке и ссылке
# Yandex Cloud API (Vision и YandexGPT)
# Получите на: https://console.cloud.yandex.ru/folders/
YANDEX_CLOUD_API_KEY=your_yandex_api_key_here
YANDEX_CLOUD_FOLDER_ID=your_yandex_folder_id_here
# Для YandexGPT (опционально, если используете Pro версию)
YANDEX_GPT_MODEL_URI=gpt://your_folder_id/yandexgpt-lite/latest

# Anthropic Claude API (для парсинга сайтов)
# Получите на: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# ScraperAPI (опционально, для обхода защиты сайтов)
# Получите на: https://www.scraperapi.com/
SCRAPER_API_KEY=your_scraper_api_key_here
